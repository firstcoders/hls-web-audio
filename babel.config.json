{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": [">0.25%, not dead", "not ie 11", "not op_mini all"]
        },
        "debug": false,
        "modules": "commonjs",
        "exclude": ["transform-regenerator"]
      }
    ]
  ],
  "plugins": ["@babel/plugin-proposal-class-properties"],
  "env": {
    "test": {
      "plugins": [
        [
          "istanbul",
          {
            "include": ["**/src"],
            "exclude": ["**/test", "**/dist", "**/node_modules/**", "**/src/util/observer*"]
          }
        ]
      ]
    }
  }
}
